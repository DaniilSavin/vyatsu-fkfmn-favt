else if(oneClick.ExportSetting.Format==='REFWORKS'){directExportTitle=Export.REFWORK_EXPORT_ALT();}
else if(oneClick.ExportSetting.Format==='Mendeley'){directExportTitle=Export.SAVE_TO_MENDELEY_ALT();}
