else if(authFormName=='AuthorLookupResultsForm'&&checkOrcidEmpty==false){if(orcidPresenceCheck||jQuery('#orcidId').length>0){document.AuthorLookupResultsForm.orcidId.value="";}
if(allowAffilProfiling){var institute;if(document.AuthorLookupResultsForm.affilName!==undefined){institute=document.AuthorLookupResultsForm.affilName;}else{institute=document.AuthorLookupResultsForm.institute;}
returnStatus=checkSearchBoxesAuthor([document.AuthorLookupResultsForm.st1,institute],true);}else if(!(allowAffilProfiling)){returnStatus=checkSearchBoxesAuthor([document.AuthorLookupResultsForm.st1],false);}}
if(returnStatus==true){if(authFormName=='AuthorLookupValidatedSearchForm'){document.AuthorLookupValidatedSearchForm.submit();}
else if(authFormName=='AuthorLookupResultsForm'){if(!authorAffilSelection){document.AuthorLookupResultsForm.submit();}else{openPopUpWindow('','authorWin',900,550,3,1,1,0,1,1,0);document.AuthorLookupResultsForm.submit();$('#authAffiliationModal').modal('hide');}}}else{jQuery('form[name="'+authFormName+'"]').off('submit').on('submit',function(e){e.preventDefault();e.stopPropagation();});}}
function clearAuthFormValue(authFormName){if(authFormName=='AuthorLookupResultsForm'){document.AuthorLookupResultsForm.st1.value="";document.AuthorLookupResultsForm.st2.value="";}
else if(authFormName=='AuthorLookupValidatedSearchForm'){document.AuthorLookupValidatedSearchForm.searchterm1.value="";document.AuthorLookupValidatedSearchForm.searchterm2.value="";}}
function checkOrcidField(orcidValue)
{var orcidName=jQuery('#orcidId').attr("name");var success=true;if(assertNotEmptyString(orcidValue)==false){if(success){if(assertNotEmptyString(orcidName)==true){msg=authorResult.JS_AUTHOR_NO_ORCID_MSG();}else{msg=authorResult.JS_AUTHOR_RESULT_NOT_FIELDS_2();}
jQuery("#orcidId").focus();success=false;}}else{success=true;}
if(!success){jQuery("#error-orcidId").html(msg).removeClass("hidden");}
return success;}
function checkOrcidValid(orcidValue){orcidValue=orcidValue.toLowerCase().replace(/[^x0-9]/g,'');if(orcidValue.length>=16){orcidValue=orcidValue.substring(0,15).replace(/\D/g,'')+orcidValue.substring(15);if(orcidValue.length==16){var newStr=orcidValue.replace(/(.{4})/g,"$1-");var newtemp=newStr.slice(0,-1);jQuery("#orcidId").val(newtemp);return true;}}
jQuery("#error-orcidId").html(error.js.message.search.syntax.orcid.format.message()).removeClass("hidden");jQuery("#orcidId").focus();return false;}
function toDetails(form,id,offset,st1,st2){form.details.value=id;form.offset.value=offset;form.st1.value=st1;form.st2.value=st2;form.submit();return false;}
function checkSubmit(){success=true;if((assertNotEmpty(document.AuthorLookupResultsForm.affilName)==false)){alert(authorResult.JS_AUTHOR_RESULT_NO_AFFILIATION_3());success=false;}else{document.AuthorLookupResultsForm.affilName.focus();}
return success;}
