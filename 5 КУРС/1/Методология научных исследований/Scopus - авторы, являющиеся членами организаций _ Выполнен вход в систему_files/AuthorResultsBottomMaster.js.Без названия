function validatePopupSelectionCount(pageId){var ids;var totalCnt=0;var isAllSelected;if(pageId==authorPageId){ids=document.AuthorLookupResultsForm.authorIds;selectedItems=document.AuthorLookupResultsForm.selectedResultsCount.value;totalCnt=document.AuthorLookupResultsForm.previousResultCount.value;isAllSelected=document.AuthorLookupResultsForm.allField.checked;if(!isAuthorsChecked(document.AuthorLookupResultsForm)&&selectedItems==0&&!isAllSelected){alert(authorResult.JS_AUTHOR_NO_AUTHORS_ADDTOSEARCH());return false;}}else if(pageId==authorListPageId){ids=document.AuthorListResultsForm.authorIds;selectedItems=document.AuthorListResultsForm.selectedResultsCount.value;totalCnt=document.AuthorListResultsForm.previousResultCount.value;isAllSelected=document.AuthorListResultsForm.allField.checked;if(!isAuthorsChecked(document.AuthorListResultsForm)&&selectedItems==0&&!isAllSelected){alert(authorResult.JS_AUTHOR_NO_AUTHORS_ADDTOSEARCH());return false;}}else{ids=document.AffiliationLookupResultsForm.affiliationIds;totalCnt=document.AffiliationLookupResultsForm.previousResultCount.value;isAllSelected=document.AffiliationLookupResultsForm.allField.checked;selectedItems=document.AffiliationLookupResultsForm.selectedResultsCount.value;if(!isAffiliationsChecked()&&selectedItems==0&&!isAllSelected){alert(authorResult.JS_AUTHOR_NO_AFFILIATIONS_ADDTOSEARCH());return false;}}
tooManySelectedErrorMsg=authorResult.JS_AUTHOR_TOO_MANY_SELECTED(maxProfileSelectionLimit);return checkForTooManySelections(tooManySelectedErrorMsg,previousSelectedCount,ids,maxProfileSelectionLimit,isAllSelected,totalCnt);}
function checkForTooManySelections(errorMessage,previousSelectionCount,selectedArray,limit,isAllSelected,totalCnt){var currentSelectionCount=0;if(isAllSelected==true||isAllSelected=="on"){currentSelectionCount=parseInt(totalCnt)+parseInt(previousSelectionCount);}else{for(var i=0;i<selectedArray.length;i++){if(selectedArray[i].checked==true){currentSelectionCount++;}}
currentSelectionCount=parseInt(currentSelectionCount)+parseInt(previousSelectionCount);}
var returnValue=true;if(currentSelectionCount>limit||(currentSelectionCount+pageToPageSelections)>limit){returnValue=confirm(errorMessage);}
if(returnValue==false){previousSelectionCount="0";}
return returnValue;}
function validateZeroSelections(pageId){var ids;var totalCnt;var isAllSelected;var selectedItems;var myForm;if(pageId==affiliationPageId){myForm=document.AffiliationLookupResultsForm;ids=myForm.affiliationIds;totalCnt=myForm.previousResultCount.value;isAllSelected=myForm.allField.checked;selectedItems=myForm.selectedResultsCount.value;if(!isAffiliationsChecked()&&selectedItems==0&&!isAllSelected){alert(authorResult.JS_AUTHOR_NO_AFFILIATIONS_DOCUMENTS());return false;}}else if(pageId==authorPageId){myForm=document.AuthorLookupResultsForm;ids=myForm.authorIds;totalCnt=myForm.previousResultCount.value;isAllSelected=myForm.allField.checked;selectedItems=myForm.selectedResultsCount.value;if(!isAuthorsChecked(document.AuthorLookupResultsForm)&&selectedItems==0&&!isAllSelected){if(isCreateFeed==true){alert(authorResult.JS_AUTHOR_NO_AUTHORS_SETFEED());}else{alert(authorResult.JS_AUTHOR_NO_AUTHORS_CONTINUE());}
return false;}}else if(pageId===authorListPageId){myForm=document.AuthorListResultsForm;ids=myForm.authorIds;totalCnt=myForm.previousResultCount.value;isAllSelected=myForm.allField.checked;selectedItems=myForm.selectedResultsCount.value;if(!isAuthorsChecked(document.AuthorListResultsForm)&&selectedItems==0&&!isAllSelected){if(isCreateFeed==true){alert(authorResult.JS_AUTHOR_NO_AUTHORS_SETFEED());}else{alert(authorResult.JS_AUTHOR_NO_AUTHORS_CONTINUE());}
return false;}}else{myForm=document.CoAuthorResultsForm;ids=myForm.authorIds;totalCnt=myForm.previousResultCount.value;isAllSelected=myForm.allField.checked;selectedItems=myForm.selectedResultsCount.value;if(!isCoAuthorsChecked()&&selectedItems==0&&!isAllSelected){if(isCreateFeed==true){alert(authorResult.JS_AUTHOR_NO_AUTHORS_SETFEED());}else{alert(authorResult.JS_AUTHOR_NO_AUTHORS_CONTINUE());}
return false;}}
return true;}
function validateCreateFeed(pageId){var totalCnt;var isAllSelected;var ids;isCreateFeed=true;ids=document.AuthorLookupResultsForm.authorIds;totalCnt=document.AuthorLookupResultsForm.previousResultCount.value;isAllSelected=document.AuthorLookupResultsForm.allField.checked;document.AuthorLookupResultsForm.target="_blank";tooManySelectedErrorMsg=authorResult.JS_AUTHOR_TOO_MANY_SELECTED(maxProfileSelectionLimit);return validateZeroSelections(pageId)&&checkForTooManySelections(tooManySelectedErrorMsg,previousSelectedCount,ids,maxProfileSelectionLimit,isAllSelected,totalCnt);}
function validateShowDocuments(pageId){var totalCnt;var isAllSelected;var ids;if(pageId==affiliationPageId){ids=document.AffiliationLookupResultsForm.affiliationIds;totalCnt=document.AffiliationLookupResultsForm.previousResultCount.value;isAllSelected=document.AffiliationLookupResultsForm.allField.checked;}else if(pageId==authorPageId){ids=document.AuthorLookupResultsForm.authorIds;totalCnt=document.AuthorLookupResultsForm.previousResultCount.value;isAllSelected=document.AuthorLookupResultsForm.allField.checked;}else if(pageId==authorListPageId){ids=document.AuthorListResultsForm.authorIds;totalCnt=document.AuthorListResultsForm.previousResultCount.value;isAllSelected=document.AuthorListResultsForm.allField.checked;}else{ids=document.CoAuthorResultsForm.authorIds;totalCnt=document.CoAuthorResultsForm.previousResultCount.value;isAllSelected=document.CoAuthorResultsForm.allField.checked;}
tooManySelectedErrorMsg=authorResult.JS_AUTHOR_TOO_MANY_SELECTED(maxProfileSelectionLimit);return validateZeroSelections(pageId)&&checkForTooManySelections(tooManySelectedErrorMsg,previousSelectedCount,ids,maxProfileSelectionLimit,isAllSelected,totalCnt);}
function validateSaveToList(pageId){var totalCnt;var isAllSelected;var ids;if(pageId===affiliationPageId){ids=document.AffiliationLookupResultsForm.affiliationIds;totalCnt=document.AffiliationLookupResultsForm.previousResultCount.value;isAllSelected=document.AffiliationLookupResultsForm.allField.checked;}else if(pageId===authorPageId){ids=document.AuthorLookupResultsForm.authorIds;totalCnt=document.AuthorLookupResultsForm.previousResultCount.value;isAllSelected=document.AuthorLookupResultsForm.allField.checked;}else if(pageId===authorListPageId){ids=document.AuthorListResultsForm.authorIds;totalCnt=document.AuthorListResultsForm.previousResultCount.value;isAllSelected=document.AuthorListResultsForm.allField.checked;}else{ids=document.CoAuthorResultsForm.authorIds;totalCnt=document.CoAuthorResultsForm.previousResultCount.value;isAllSelected=document.CoAuthorResultsForm.allField.checked;}
