function showTitleFailure(request,response){addlAuthors.innerHTML="<span class='txtRed'>The system is currently unable to retrieve the last title.</span><br><span class='txt'>Please try again by clicking <strong>Show Last Title</strong>.</span>";authorIdRetrievingDiv.style.display="none";authorIdShowDiv.style.display="block";}
function showTitleException(request,response){addlAuthors.innerHTML="<span class='txtRed'>The system is currently unable to retrieve the last title.</span><br><span class='txt'>Please try again by clicking <strong>Show Last Title</strong>.</span>";authorIdRetrievingDiv.style.display="none";authorIdShowDiv.style.display="block";}
function showTitleSuccess(request,response){authorIdRetrievingDiv.style.display="none";authorIdDiv.style.display="block";authorIdHideDiv.style.display="block";}
function toggleShowTitle(authorID,row){if(!lastTitle[authorID]){var retreivalURL=[lastTitle.retrieveURL,authorID].join('');jQuery.ajax({url:retreivalURL,dataType:"html",success:function(data,status){lastTitle.success(data,status,{authId:authorID,row:row});},error:function(data,status){lastTitle.error(data,status,{authId:authorID,row:row});}});}else{lastTitle.toggle(authorID,row);}
return false;}
