function getSelectedAuthorIds(){var selectedAuthorIds=[];for(var property in selectedAuthors){if(selectedAuthors.hasOwnProperty(property)){selectedAuthorIds.push(property);}}
return selectedAuthorIds;}
function navigateToAfw(){jQuery("#authorMergeCorrectionsModal").modal('hide');var url;var isOwnProfile=jQuery("#reviewDocumentPrefModal input[name='ownProfile']:checked").val();var authClaimAction=jQuery("#reviewDocumentPrefModal input[name='authClaimAction']:checked").val();var authExpoTyp=jQuery("#reviewDocumentPrefModal input[name='authCorrectionType']").val();var authNamVarsForSelAuth=jQuery("#reviewDocumentPrefModal input[name='authNamVarsForSelAuth']").val();var preferredName=selectedNameVariant;var returnUrl=window.location.href.substring(window.location.href.indexOf(window.location.pathname));var form=document.createElement('form');form.setAttribute('method','POST');form.setAttribute('action','/feedback/author/fromProfile.uri');addHiddenField(form,'isOwnProfile',isOwnProfile);addHiddenField(form,'authorIds',getSelectedAuthorIds());addHiddenField(form,'targetAuthorId',targetAuthorId);addHiddenField(form,'authClaimAction',authClaimAction);addHiddenField(form,'preferredNameFormat',htmlEscape(preferredName));addHiddenField(form,'returnToLabelKey','AFW_BACK_TO_AUTHOR_PROFILE');addHiddenField(form,'returnToUrl',returnUrl);addHiddenField(form,'authCorrectionType',authExpoTyp);addHiddenField(form,'authNamVarsForSelAuth',htmlEscape(authNamVarsForSelAuth));document.body.appendChild(form);form.submit();}
function addHiddenField(form,fieldName,fieldValue){var hiddenField=document.createElement('input');hiddenField.setAttribute('type','hidden');hiddenField.setAttribute('name',fieldName);hiddenField.setAttribute('value',fieldValue);form.appendChild(hiddenField);}
function htmlEscape(s){return s.replace(/&/g,'&amp;').replace(/</g,'&lt;').replace(/>/g,'&gt;').replace(/"/g,'&quot;').replace(/'/g,'&#039;');}
function proceedToNextPage(event){event.preventDefault();var isOwnProfile=jQuery("#reviewDocumentPrefModal input[name='ownProfile']:checked").val();var origin=$("#origin").val();if(origin!==undefined&&(origin==="AuthorNamesList"||origin==="searchauthorfreelookup")){jQuery("#preferredNameFormat").val(selectedNameVariant);jQuery("#isOwnProfile").val(isOwnProfile);jQuery("#addActiveFlag").val("true");if(document.AuthorFeedbackResultsForm!==undefined){document.AuthorFeedbackResultsForm.cl.value='t';}
if(document.AuthorLookupResultsForm!==undefined){document.AuthorLookupResultsForm.cl.value='t';}
jQuery("#authorResultsFormId").attr("target",'_self').attr("action",saveValuesUri).submit();}else{navigateToAfw();}}
function authClaimClick(event){event.preventDefault();authClaimReq.done(function(authClaimRes){if(isAuthClaimPopupNeeded(authClaimRes,true)){var modalObj=jQuery("#reviewDocumentPrefModal");if(existingAuthClaims.length>1){jQuery("#heading3 h4").html(authorResult.JS_AUTHOR.AFW_AUTH_RESULTS_AUTHCLAIM_HEADER_TEXT_PLURAL());jQuery('label[for="prevAssoc"]').html(authorResult.JS_AUTHOR.AFW_AUTH_RESULTS_AUTHCLAIM_PREV_PROFILE_RADIO_LABEL_PLURAL());jQuery('label[for="newAssoc"]').html(authorResult.JS_AUTHOR.AFW_AUTH_RESULTS_AUTHCLAIM_NEW_PROFILE_RADIO_LABEL_PLURAL());}
var prevAuthClaimInfo="";var authorName;jQuery.each(existingAuthClaims,function(index,existingAuthClaim){if(index>0){prevAuthClaimInfo+=SEMICOLON;}
authorName=existingAuthClaim.authorPreferredName.replace(/\s/g,'&nbsp;');prevAuthClaimInfo+=authorResult.JS_AUTHOR.AFW_AUTH_CLAIM_INFO(authorName,existingAuthClaim.authorId);});modalObj.find("#prevAuthClaim").html(prevAuthClaimInfo);authorName=selectedNameVariant.replace(/\s/g,'&nbsp;');var newAuthClaimInfo=authorName;if(targetAuthorId!==undefined){newAuthClaimInfo=authorResult.JS_AUTHOR.AFW_AUTH_CLAIM_INFO(authorName,targetAuthorId);}else if(selectedAuthors.length===1){var selectedAuthorIds=getSelectedAuthorIds();newAuthClaimInfo=authorResult.JS_AUTHOR.AFW_AUTH_CLAIM_INFO(authorName,selectedAuthorIds[0]);}
modalObj.find("#newAuthClaim").html(newAuthClaimInfo);modalObj.find("#content1, #content2").addClass("hidden");modalObj.find("#heading1, #heading2").addClass("hidden");modalObj.find("#content3").removeClass("hidden");modalObj.find("#heading3").removeClass("hidden");jQuery("#reviewDocumentPrefModal").focus();jQuery("#prefContinue").off("click.afw").on('click.afw',proceedToNextPage);}else{proceedToNextPage(event);}});}
function isAuthClaimPopupNeeded(authClaimRes,shouldCheckOwnProfile){var popupNeeded=false;var isOwnProfile="yes";if(authClaimRes&&authClaimRes.authors&&authClaimRes.authors.length>0){var associatedWithSelectedProfile=false;if(targetAuthorId!==undefined){jQuery.each(authClaimRes.authors,function(){if(targetAuthorId===this.authorId){associatedWithSelectedProfile=true;}});}else{var selectedAuthorIds=getSelectedAuthorIds();jQuery.each(authClaimRes.authors,function(){for(var i=0;i<selectedAuthorIds.length;i++){if(selectedAuthorIds[i]===this.authorId){associatedWithSelectedProfile=true;}};});}
if(shouldCheckOwnProfile===true){isOwnProfile=jQuery("#reviewDocumentPrefModal input[name='ownProfile']:checked").val();}
if((isOwnProfile==="yes"&&!associatedWithSelectedProfile)||(isOwnProfile==="no"&&associatedWithSelectedProfile)){popupNeeded=true;}
existingAuthClaims=authClaimRes.authors;}else if(shouldCheckOwnProfile===false){popupNeeded=true;}
return popupNeeded;}
function setPreferredNameClick(event){event.preventDefault();selectedNameVariant=jQuery('#selectedAuthList').val();authClaimReq.done(function(authClaimRes){if(isAuthClaimPopupNeeded(authClaimRes,false)){var modalObj=jQuery("#reviewDocumentPrefModal");modalObj.find("#selNameVar").text(selectedNameVariant);modalObj.find("#content1, #content3").addClass("hidden");modalObj.find("#heading1, #heading3").addClass("hidden");modalObj.find("#content2").removeClass("hidden");modalObj.find("#heading2").removeClass("hidden");modalObj.find(".modal-content").attr({"aria-live":"assertive","aria-atomic":"true","aria-relevant":"all"});jQuery("#reviewDocumentPrefModal").focus();jQuery("#prefContinue").off("click.afw").on('click.afw',authClaimClick);}else{proceedToNextPage(event);}});}
function setPreferredNamePopupContent(modalObj){var $selectObj=jQuery('#selectedAuthList');$selectObj.find('option').remove();$selectObj.addClass('mediumSelect selectmenu');var nameVariants=[];var selectedAuthorIds=getSelectedAuthorIds();for(var i=0;i<selectedAuthorIds.length;i++){var selectedAuthorNames=selectedAuthors[selectedAuthorIds[i]];for(var j=0;j<selectedAuthorNames.length;j++){nameVariants.push(selectedAuthorNames[j]);}}
