function setCurrentAllPageSelection(state){var tooltip='';var boundCheckboxId='';if(state==='All'){tooltip=myMessages.allSelectTooltip;boundCheckboxId=selectAllCheckboxSelector;}else if(state==='Page'){tooltip=myMessages.pageSelectTooltip;boundCheckboxId=selectPageCheckboxSelector;}
