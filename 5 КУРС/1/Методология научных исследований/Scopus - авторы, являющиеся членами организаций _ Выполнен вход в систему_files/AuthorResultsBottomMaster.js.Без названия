function checkTheForm(name){var authFormName=name;var returnStatus=false;var authorAffilSelection=jQuery("input[name=authorAFfilSelection]").val();jQuery('#orcidErrorMsgContainer').remove();if(selectOrcidEmpty==true){insertAreaId="orcidSelectSearch";}
