function getQueryParamFromUrl(name,url){if(!url){url=window.location.href;}
var regex=new RegExp("[?&]"+name+"(?:=([^&]*)|$)");var results=regex.exec(url);if(results){return decodeURIComponent(results[1].replace(/\+/g," "));}
return null;}
