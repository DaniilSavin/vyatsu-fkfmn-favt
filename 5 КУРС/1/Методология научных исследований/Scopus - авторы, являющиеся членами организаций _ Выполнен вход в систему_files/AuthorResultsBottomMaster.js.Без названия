function listSetUpSave(event){event.preventDefault();MessageContainer.removeMsgContatiner();var inputValTrim=jQuery.trim(jQuery('#listName').val());var listSelect=jQuery("#addListId");var oldValue=jQuery('#myListId').val();if(oldValue==null){oldValue=-1;}
var SaveToListData={listFormObj:jQuery("#ListForm"),ajaxURL:"/list/savelist/add.uri"};if(SaveToListData.listFormObj.length===0){SaveToListData.listFormObj=jQuery("#searchResFormId");}
