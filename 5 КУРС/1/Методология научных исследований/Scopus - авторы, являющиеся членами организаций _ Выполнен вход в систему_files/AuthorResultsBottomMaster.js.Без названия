function onResultClick(checkbox){if(!checkbox.checked){jQuery(checkbox).removeAttr('checked');jQuery(selectAllCheckboxSelector).attr("checked",false);jQuery(selectAllOrPageCheckboxSelector).attr("checked",false);jQuery(selectPageCheckboxSelector).attr("checked",false);isPageSelected=false;}else{jQuery(checkbox).attr('checked','checked');if(allResultsOnPageAreSelected()){jQuery(selectPageCheckboxSelector).attr("checked",!allOffPageAreSelected);jQuery(selectAllCheckboxSelector).attr("checked",allOffPageAreSelected);jQuery(selectAllOrPageCheckboxSelector).attr("checked",true);isPageSelected=true;setPageAllTxt();}}
