var advanceSearchWindow;function checkAdvancedSearchWindow(backToURL){var parentLocation;try{if(window.opener){parentLocation=window.opener.location.href;}}catch(locationError){return openPopUpWindow(backToURL,'advanceSearchWin',800,600,5,1,1,1,1,1,1);}
if(parentLocation==null||parentLocation.length==0){return openPopUpWindow(backToURL,'advanceSearchWin',800,600,5,1,1,1,1,1,1);}else if(parentLocation!=null&&parentLocation.indexOf('display=advanced')==-1&&parentLocation.indexOf('display=edit')==-1&&window.opener.document.AdvancedValidatedSearchForm==null){window.opener.location.href=backToURL;return window.opener;}
return window.opener;}
function isAdvanceSearchLoaded(){if(advanceSearchWindow.document.AdvancedValidatedSearchForm!=null){if(advanceSearchWindow.document.AdvancedValidatedSearchForm.searchfield!=null){return true;}}
return false;}
function waitToLoad(query){if(!isAdvanceSearchLoaded()){setTimeout(function(){waitToLoad(query);},500);}
if(isAdvanceSearchLoaded()){setAllQueryOnAdvanceSearch(query);clearTimeout();}}
function setAllQueryOnAdvanceSearch(query){var popupQuery=query;popupQuery=popupQuery.replace(/&#034;/g,"\"");popupQuery=popupQuery.replace(/&#039;/g,"'");var searchObj=jQuery('#searchfield',advanceSearchWindow.document);var outlineObj=jQuery('#outlineButton',advanceSearchWindow.document);jQuery('.searchForTxtDiv',advanceSearchWindow.document).css({'display':'none'});outlineObj.attr("disabled",false);outlineObj.removeClass("outlineDisabled");outlineObj.addClass("outlineEnabled");jQuery('#clearLink',advanceSearchWindow.document).css('display','inline');jQuery('#clearLinkDiv',advanceSearchWindow.document).css('display','inline');jQuery('#searchDivider:visible',advanceSearchWindow.document).hide();jQuery('#compactQueryLink:visible',advanceSearchWindow.document).hide();var searchText=searchObj.text();searchText=jQuery.trim(searchText);if(searchText!=null&&searchText!=""){var tempQuery="";searchText=tempQuery.concat("(",searchText);searchText=searchText.concat(")"," AND ");popupQuery=searchText.concat(popupQuery);}
searchObj.html(popupQuery);searchObj.focus();window.close();}
