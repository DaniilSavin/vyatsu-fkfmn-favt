function validatePopupSelection(count,pageId){if(count==0&&pageId==authorPageId){if(document.AuthorLookupResultsForm.popupSubmit.value=="true"){alert('No authors were selected.\r\nPlease select one or more authors from the list below and click add to search.');resetPopupSubmitFlag(pageId);return;}}else if(count==0&&pageId==affiliationPageId){if(document.AffiliationLookupResultsForm.popupSubmit.value=="true"){alert('No affiliations were selected.\r\nPlease select one or more affiliations from the list below and click add to search.');resetPopupSubmitFlag(pageId);return;}}else if(count==0&&pageId==coAuthorPageId){if(document.CoAuthorResultsForm.popupSubmit.value=="true"){alert('No affiliations were selected.\r\nPlease select one or more affiliations from the list below and click add to search.');resetPopupSubmitFlag(pageId);return;}}}
function resetPopupSubmitFlag(pageId){if(pageId==authorPageId){document.AuthorLookupResultsForm.popupSubmit.value="false";}else if(pageId==affiliationPageId){document.AffiliationLookupResultsForm.popupSubmit.value="false";}else{document.CoAuthorResultsForm.popupSubmit.value="false";}}
