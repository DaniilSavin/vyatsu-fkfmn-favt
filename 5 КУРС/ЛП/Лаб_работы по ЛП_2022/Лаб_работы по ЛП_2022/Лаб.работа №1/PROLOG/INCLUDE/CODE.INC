%  include "d:\\prolog\\include\\repeat.inc"

  DATABASE-code
    tb(INTEGER,INTEGER)

  PREDICATES
    code(SYMBOL,SYMBOL)
    decode(SYMBOL,SYMBOL)
    
  CLAUSES
    decode("",""):-!.
    decode(CodeStr,Str):-
      frontchar(CodeStr,Char,Rest),!,
      decode(Rest,Work),!,
      char_int(Char,Int),!,
      tb(Int1,Int),!,
      char_int(Char1,Int1),!,
      str_char(S1,Char1),!,
      concat(S1,Work,Str),!.

    code("",""):-!.
    code(Str,CodeStr):-
      frontchar(Str,Char,Rest),!,
      code(Rest,Work),!,
      char_int(Char,Int),!,
      tb(Int,Int1),!,
      char_int(Char1,Int1),!,
      str_char(S1,Char1),!,
      concat(S1,Work,CodeStr),!.
      
